<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Calendar" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#378F8F</item>
        <item name="colorPrimaryVariant">#B5B5B5</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <!-- Customize your theme here. -->
    </style>
    <style name="ActionBarTitleText" parent="@style/Widget.MaterialComponents.Toolbar">
        <item name="android:textSize">25sp</item>
        <item name="android:textColor" >#200000</item>
    </style>
    <style name="FullScreenDialog" parent="Theme.MaterialComponents.Light.Dialog">

        <item name="android:windowIsFloating">false</item>
        <item name="android:windowBackground">@color/white</item>

    </style>
    <style name="FullScreenDialogColor" parent="FullScreenDialog">
    </style>
</resources>